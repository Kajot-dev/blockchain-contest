{
  "name": "hardhat-nft-development",
  "license": "UNLICENSED",
  "scripts": {
    "block": "hardhat block-number --network localhost",
    "add-stores": "hardhat run scripts/S01-addingVendors.js --network localhost",
    "mint-nfts": "hardhat run scripts/N01-NikeNFTMint.js --network localhost",
    "mp-list": "hardhat run scripts/M01-Listing.js --network localhost",
    "mp-buy": "hardhat run scripts/M02-Buying.js --network localhost",
    "mp-cancel": "hardhat run scripts/M03-Cancel.js --network localhost",
    "mp-update": "hardhat run scripts/M04-Updating.js --network localhost",
    "run-base": "yarn add-stores && yarn mint-nfts && yarn mp-list && yarn block",
    "run-withdraw": "yarn run-base && yarn mp-buy && && yarn mp-withdraw yarn block"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "chai": "^4.2.0",
    "ethers": "5.7.2",
    "hardhat": "^2.14.0",
    "hardhat-deploy": "^0.11.28",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.0",
    "typechain": "^8.1.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.3",
    "dotenv": "^16.0.3"
  },
  "repository": "https://github.com/krrdash/hardhat-development",
  "author": "krrdash <jpomianowski@outlook.com>"
}
